<haakjeopenen.phapp.structalikes.MultiSwipeRefreshLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/swipe_container_photos"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <GridView
        android:id="@+id/photosGridView"
        android:name="haakjeopenen.phapp.PhotosFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:columnWidth="90dp"
        android:numColumns="auto_fit"
        android:verticalSpacing="5dp"
        android:horizontalSpacing="5dp"
        android:stretchMode="columnWidth"
        android:gravity="center"
        tools:context=".fragments.PhotosFragment"
        tools:listitem="@layout/fragment_photos_item"
        />
</haakjeopenen.phapp.structalikes.MultiSwipeRefreshLayout>
